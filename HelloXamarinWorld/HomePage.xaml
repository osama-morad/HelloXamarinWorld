<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloXamarinWorld.HomePage"
             xmlns:local="clr-namespace:HelloXamarinWorld">

    <TabbedPage.ToolbarItems>
        <ToolbarItem x:Name="newTravelAddButton" Text="Add" Clicked="newTravelAddButton_Clicked"></ToolbarItem>
    </TabbedPage.ToolbarItems>
    <local:HistoryPage Title="History"></local:HistoryPage>
    <local:MapPage Title="Map" ></local:MapPage>
    <local:ProfilePage Title="Profile" ></local:ProfilePage>
</TabbedPage>